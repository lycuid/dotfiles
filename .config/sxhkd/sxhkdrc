# misc.
super + p
  $LAUNCHER
super + Return
  $TERMINAL
super + ctrl + b
  $BROWSER
super + ctrl + l
  xset s activate
super + ctrl + c
  dmenu_ssh
super + ctrl + o
  dmenu_edit -d -p "open project :" $SCM
super + ctrl + d
  pkill -USR1 clubar

# system audio keybindings.
super + {_, ctrl +} equal
  volume {1, 5}%+
super + {_, ctrl +} minus
  volume {1, 5}%-
super + ctrl + m
  volume toggle

# Force shutdown, reboot.
super + ctrl + s
  dmenu_shutdown -f

# dunst
ctrl + {_, shift +} space
  dunstctl {close, close-all}
